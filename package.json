{
  "name": "suomifi-ui-components",
  "version": "0.8.0",
  "description": "Suomi.fi UI component library",
  "main": "dist/index.js",
  "module": "dist/suomifi-ui-components.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vrk-kpa/suomifi-ui-components.git"
  },
  "author": "Joonas Kallunki <jkallunki@gmail.com> (https://jkallunki.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vrk-kpa/suomifi-ui-components/issues"
  },
  "homepage": "https://github.com/vrk-kpa/suomifi-ui-components#readme",
  "scripts": {
    "prepare": "yarn run build",
    "start": "yarn styleguide",
    "start:withaxe": "cross-env AXE=\"true\" yarn styleguidist server",
    "test": "tsdx test --passWithNoTests",
    "test:updatesnapshots": "tsdx test --u",
    "lint": "npm-run-all lint:tsdx-lint lint:stylelint",
    "lint:tsdx-lint": "tsdx lint src",
    "lint:stylelint": "stylelint ./src/**/*.{ts,tsx}",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:check": "prettier-check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "validate": "npm-run-all test lint prettier:check",
    "build": "npm-run-all build:tsdx build:css",
    "build:tsdx": "tsdx build",
    "build:css": "node globalCss.js",
    "bundle-analyzer": "size-limit --why",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy": "gh-pages -d styleguide -m \"[skip ci] Updates\"",
    "deploy-dev": "gh-pages -d styleguide -e dev -m \"[skip ci] Updates\""
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@types/classnames": "2.2.6",
    "@types/jest": "25.1.1",
    "@types/jest-axe": "3.2.1",
    "@types/react": "16.8.0",
    "@types/react-dom": "16.8.0",
    "@types/styled-components": "4.1.18",
    "@types/uuid": "3.4.5",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.x",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.0.3",
    "clean-css": "4.2.3",
    "clipboard-copy": "3.0.0",
    "cross-env": "5.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-tree-shaking": "1.8.0",
    "gh-pages": "2.0.1",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "jest-axe": "3.3.0",
    "jest-styled-components": "6.3.4",
    "lint-staged": "8.0.5",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "prettier-check": "2.0.0",
    "prettier-eslint": "9.0.1",
    "prop-types": "15.7.2",
    "react": "16.8.0",
    "react-axe": "3.3.0",
    "react-docgen": "2.21.0",
    "react-docgen-typescript": "1.12.2",
    "react-dom": "16.8.0",
    "react-styleguidist": "9.1.13",
    "rollup-plugin-postcss": "2.0.5",
    "size-limit": "1.3.2",
    "styled-components": "4.3.2",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-processor-styled-components": "1.6.0",
    "ts-jest": "25.2.0",
    "tsdx": "0.12.3",
    "tslib": "1.10.0",
    "tslint-webpack-plugin": "2.1.0",
    "typescript": "3.3.1"
  },
  "dependencies": {
    "@reach/menu-button": "0.8.5",
    "@reach/popover": "0.8.5",
    "classnames": "2.2.6",
    "normalize.cssinjs": "1.1.0",
    "polished": "3.4.0",
    "react-svg": "7.2.2",
    "suomifi-design-tokens": "0.7.1",
    "suomifi-icons": "0.1.0",
    "uuid": "3.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "styled-components": ">=4.3.2"
  },
  "style": "dist/main.css",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all lint prettier:check",
      "pre-push": "yarn run validate"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier-check"
    ]
  },
  "size-limit": [
    {
      "path": "dist/suomifi-ui-components.esm.js"
    }
  ]
}
